{
    "name": "bostonarnold19/union",
    "description": "Laravel wrapper around OAuth 1 & OAuth 2 libraries.",
    "keywords": ["oauth", "laravel"],
    "license": "MIT",
    "homepage": "https://boston.io",
    "support": {
        "issues": "https://github.com/bostonarnold19/union/issues",
        "source": "https://github.com/bostonarnold19/union"
    },
    "authors": [
        {
            "name": "Arnold Boston",
            "email": "bostonarnold1234567890@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/contracts": "~5.7",
        "illuminate/http": "~5.7",
        "illuminate/support": "~5.7",
        "league/oauth1-client": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "BostonIO\\Union\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BostonIO\\Union\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "BostonIO\\Union\\UnionServiceProvider"
            ],
            "aliases": {
                "Union": "BostonIO\\Union\\Facades\\Union"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
